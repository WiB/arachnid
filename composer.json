{
    "name": "zrashwani/arachnid",
    "type": "library",
    "description": "A crawler to find all unique internal pages on a given website",
    "keywords": ["crawl","spider","search","scrape"],
    "homepage": "http://github.com/zrashwani/arachnid",
    "license": "MIT",
    "authors": [
        {
            "name": "Zeid Rashwani",
            "homepage": "http://zrashwani.com"
        },
        {
            "name": "Josh Lockhart",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "monolog/monolog": "^1.22",
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4",
        "symfony/panther": "^0.2.0",
        "fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^2.7",
        "squizlabs/php_codesniffer": "3.2.*",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Arachnid\\": ["src/Arachnid/", "tests/src/"]
        }
    }
}
